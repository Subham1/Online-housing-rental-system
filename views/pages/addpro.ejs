<div class="addpro-bg">`

    <div class="row pt-5">
        <div class="col-12">
            <div class="text-center text-bold text-light">
                
            </div>
        </div>
    </div>


    <div class="container pb-4 pt-2 h-100" >
        <div class="row justify-content-center align-items-center h-100">
          
          <div class="col-12 col-lg-9 col-xl-7">
            
            <div class="card shadow-2-strong card-registration" style="border-radius: 26px;">
              <div class="addpro-body">
              <div class="card-body p-3 p-md-5">
                <h3 class="mb-4 pb-1 pb-md-0 mb-md-5">Add Property</h3>
                
                  
                  <div class="row">
                    
                    <div class="col-md-6 mb-4">
                         <form action="/addpro" method="post" enctype="multipart/form-data">
                      <div class="form-outline">
                        <label class="form-label " for="firstName" aria-placeholder="House/Apartment name">House/Apartment name</label>
                        <input type="text" id="firstName" class="form-control form-control-lg" name="housename"  />
                        
                      </div>
    
                    </div>
                    <div class="col-md-6 mb-4">
    
                      <div class="form-outline">
                        <label class="form-label" for="lastName" aria-placeholder="Flat/House No">Flat/House No</label>
                        <input type="text" id="lastName" class="form-control form-control-lg" name="houseno" />
                        
                      </div>
    
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-6 mb-4 pb-2">
    
                      <div class="form-outline">
                        <label class="form-label" for="address">Addres</label>
                        <input type="text" id="address" class="form-control form-control-lg" name="address"/>
                        
                      </div>
    
                    </div>
                    <div class="col-md-6 mb-4 pb-2">
    
                      <div class="form-outline">
                        <label class="form-label" for="PINCODE">PINCODE</label>
                        <input type="tel" id="PINCODE" class="form-control form-control-lg" name="pincode"/>
                        
                      </div>
    
                    </div>
                  </div>
    
                  <div class="row">
                    <div class="col-md-4 mb-4 d-flex align-items-center">
    
                      <div class="form-outline datepicker w-100">
                        <label for="type" class="form-label">Type</label>
                        <select  id="type" name="type">
                          <option value="1BHK">1BHK</option>
                          <option value="2BHK">2BHK</option>
                          <option value="3BHK">3BHK</option>
                          <option value="others">other</option>
                        </select>
                       
                      </div>
    
                    </div>
                    <div class="col-md-4 mb-4 d-flex align-items-center">
    
                      <div class="form-outline datepicker w-100">
                        <label for="price" class="form-label">PRICE(monthly)</label>
                        <select  id="price" name="price">
                          <option value="0-3k">0-3k</option>
                          <option value="0-3k">3-6k</option>
                          <option value="0-3k">6-8k</option>
                          <option value="3k-7k">8k-10k</option>
                          <option value="7k-12k">10-14k</option>
                          <option value="12k-20k">14k-abovek</option>
                        </select>
                       
                      </div>
    
                    </div>
                    
                   <div class="col-md-4 mb-4">

             
                    <div class="form-check form-switch">
                      <input class="form-check-input" type="checkbox" id="bach" name="bach">
                      <label class="form-check-label" for="bach">Bachelor Availabe</label>
                    </div>
                   </div>
                  
                  </div>
                 <div class="row">
                  <div class="col-4 ">
                      <label for="img1" class="form-label">Image1</label>
                      <input class="form-control form-control-sm" id="img1" type="file" name="img1" accept="image/*">
                  </div>
                  <div class="col-4">
                    <label for="img2" class="form-label">Image2</label>
                    <input class="form-control form-control-sm" id="img2" type="file" name="img2" accept="image/*">
                </div>
                <div class="col-4">
                  <label for="img3" class="form-label">Image3</label>
                  <input class="form-control form-control-sm" id="img3" type="file"   name="img3"  accept="image/*">
              </div>
                 </div>
                 <div class="form-outline pt-3">
                  <label class="form-label" for="Whatsapp" aria-placeholder="">Whatsapp link</label>
                  <input type="text" id="Whatsapp" class="form-control form-control-lg" name="Whatsapp" />
                  
                </div>
    
                  <div class="mt-4 pt-2">
                    <input class="btn btn-primary btn-lg" type="submit" value="Submit" />
                  </div>
    
                </form>
              </div>
            </div>
          </div>
          </div>
        </div>
      </div>
    </div>
    </body>
    <%- include('../partials/foot') %>
</html>